{
  "name": "markdown-it-dollarmath",
  "version": "0.5.0",
  "description": "A markdown-it plugin for $-delimited math.",
  "author": "Chris Sewell",
  "homepage": "https://github.com/executablebooks/markdown-it-dollarmath",
  "license": "MIT",
  "keywords": [
    "markdown",
    "markdown-it",
    "markdown-it-plugin",
    "latex",
    "math"
  ],
  "type": "module",
  "files": [
    "src",
    "dist"
  ],
  "exports": "./dist/index.js",
  "types": "dist/index.d.ts",
  "unpkg": "dist/index.umd.min.js",
  "scripts": {
    "clean": "rimraf dist",
    "format": "prettier --write src/**/*.ts tests/**/*.ts",
    "lint": "eslint -c .eslintrc.yml --max-warnings 1 src/**/*.ts tests/**/*.ts",
    "lint:fix": "eslint -c .eslintrc.yml --fix src/**/*.ts tests/**/*.ts",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "prebuild": "rm -rf dist",
    "build:bundles": "rollup -c",
    "build:esm": "tsc --module node16 --outDir dist",
    "build": "npm-run-all -l clean -p build:esm build:bundles",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=8.3"
  },
  "peerDependencies": {
    "markdown-it": "^14"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/katex": "^0.16.7",
    "@types/markdown-it": "^14.1.2",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vitest/coverage-v8": "^2.1.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.10.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^6.6.0",
    "katex": "^0.16.11",
    "markdown-it": "^14.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "rollup": "^4.22.4",
    "rollup-plugin-typescript2": "^0.36.0",
    "typescript": "^5.6.2",
    "vitest": "^2.1.1"
  }
}
